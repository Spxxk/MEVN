{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nconst API_URL = 'http://localhost:3000';\nexport default {\n  name: 'GamesList',\n  data() {\n    return {\n      games: [],\n      filterText: '',\n      selectedLeague: '',\n      leagues: [] // Add a property to store the list of leagues\n    };\n  },\n  async created() {\n    try {\n      const response = await axios.get(`${API_URL}/event/all/`, {\n        headers: {\n          'Content-Type': 'application/x-www-form-urlencoded'\n        }\n      });\n      this.games = response.data;\n      this.extractLeagues(); // Call method to extract leagues from games data\n    } catch (error) {\n      console.error('Error fetching games:', error);\n    }\n  },\n  computed: {\n    filteredGames() {\n      let filtered = this.games;\n      if (this.selectedLeague) {\n        filtered = filtered.filter(game => game.league === this.selectedLeague);\n      }\n      if (this.filterText) {\n        filtered = filtered.filter(game => game.game.toLowerCase().includes(this.filterText.toLowerCase()));\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      return date.toDateString();\n    },\n    async deleteEvent(game) {\n      console.log(game);\n      let urll = `${API_URL}/event/${game}/`;\n      console.log(urll);\n      try {\n        const response = await axios.delete(urll);\n        console.log(response);\n      } catch (error) {\n        console.error('Error fetching games:', error);\n      }\n      location.reload();\n    },\n    async editEvent(game) {\n      this.$router.push(`/edit_event/${game}`);\n    },\n    extractLeagues() {\n      // Extract unique league names from games data\n      this.leagues = Array.from(new Set(this.games.map(game => game.league)));\n    }\n  }\n};","map":{"version":3,"names":["axios","API_URL","name","data","games","filterText","selectedLeague","leagues","created","response","get","headers","extractLeagues","error","console","computed","filteredGames","filtered","filter","game","league","toLowerCase","includes","methods","formatDate","dateString","date","Date","toDateString","deleteEvent","log","urll","delete","location","reload","editEvent","$router","push","Array","from","Set","map"],"sources":["C:\\Users\\owenc\\OneDrive\\Desktop\\owen\\frontend\\src\\components\\GamesList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"section\">\r\n      <h1 class=\"title\">Events List</h1>\r\n    </div>\r\n    <div>\r\n      <div class=\"card\" style=\"margin: 20px\">\r\n        <header class=\"card-header\" style=\"background-color: #e32363;\">\r\n          <p class=\"card-header-title\">Filter</p>\r\n        </header>\r\n        <div class=\"card-content is-flex is-justify-content-left is-flex-direction-column\">\r\n          <div class=\"content\">\r\n            <div class=\"field is-horizontal\">\r\n              <div class=\"field-label is-normal\">\r\n                <label for=\"filterLeague\" class=\"label\">Filter by League</label>\r\n              </div>\r\n              <div class=\"field-body\">\r\n                <div class=\"field\">\r\n                  <div class=\"control\">\r\n                    <div class=\"select is-fullwidth\">\r\n                      <select id=\"filterLeague\" v-model=\"selectedLeague\">\r\n                        <option value=\"\">All</option>\r\n                        <option v-for=\"league in leagues\" :key=\"league\">{{ league }}</option>\r\n                      </select>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"field is-horizontal\">\r\n            <div class=\"field-label is-normal\">\r\n              <label for=\"filterText\" class=\"label\">Filter by Event</label>\r\n            </div>\r\n            <div class=\"field-body\">\r\n              <div class=\"field\">\r\n                <div class=\"control\">\r\n                  <input class=\"input\" id=\"filterText\" type=\"text\" v-model=\"filterText\" placeholder=\"Search...\">\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"card\" style=\"margin: 20px;\">\r\n      <table class=\"table\">\r\n        <!-- Table headers -->\r\n        <thead>\r\n          <tr>\r\n            <th class=\"has-background-danger\">Event</th>\r\n            <th class=\"has-background-danger\">Type</th>\r\n            <th class=\"has-background-danger\">Location</th>\r\n            <th class=\"has-background-danger\">Date</th>\r\n            <th class=\"has-background-danger\">Description</th>\r\n            <th class=\"has-background-danger\">Team 1</th>\r\n            <th class=\"has-background-danger\">Team 2</th>\r\n            <th class=\"has-background-danger\">League</th>\r\n            <th class=\"has-background-danger\" v-if=\"this.$store.state.user.isLoggedIn\"></th>\r\n          </tr>\r\n        </thead>\r\n        <!-- Table body -->\r\n        <tbody>\r\n          <tr v-for=\"(game, index) in filteredGames\" :key=\"index\">\r\n            <td><a :href=\"'/event/' + game.game\">{{ game.game }}</a></td>\r\n            <td>{{ game.type }}</td>\r\n            <td>{{ game.location }}</td>\r\n            <td>{{ new Date(game.date).toLocaleString('en-US', {\r\n              year: 'numeric',\r\n              month: 'long',\r\n              day: '2-digit',\r\n              hour: 'numeric',\r\n              minute: 'numeric',\r\n              hour12: true,\r\n              timeZone: 'America/New_York'\r\n            })}}</td>\r\n            <td>{{ game.description }}</td>\r\n            <td>{{ game.team1 }}</td>\r\n            <td>{{ game.team2 }}</td>\r\n            <td>{{ game.league }}</td>\r\n            <td class=\"card-header has-background-danger\">\r\n              <a :href=\"'/notification/' + game.game\" class=\"button is-danger is-small\" style=\"text-decoration: none\">Reminder</a>\r\n            </td>\r\n            <td v-if=\"this.$store.state.user.isLoggedIn\">\r\n              <a @click=\"editEvent(game.game)\" class=\"button is-primary is-small\" style=\"text-decoration: none\">Edit</a>\r\n              <a @click=\"deleteEvent(game.game)\" class=\"button is-danger is-small\"\r\n                style=\"text-decoration: none\">Delete</a>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nconst API_URL = 'http://localhost:3000';\r\n\r\nexport default {\r\n  name: 'GamesList',\r\n  data() {\r\n    return {\r\n      games: [],\r\n      filterText: '',\r\n      selectedLeague: '',\r\n      leagues: [] // Add a property to store the list of leagues\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      const response = await axios.get(`${API_URL}/event/all/`, {\r\n        headers: {\r\n          'Content-Type': 'application/x-www-form-urlencoded'\r\n        }\r\n      });\r\n      this.games = response.data;\r\n      this.extractLeagues(); // Call method to extract leagues from games data\r\n    } catch (error) {\r\n      console.error('Error fetching games:', error);\r\n    }\r\n  },\r\n  computed: {\r\n    filteredGames() {\r\n      let filtered = this.games;\r\n      if (this.selectedLeague) {\r\n        filtered = filtered.filter(game => game.league === this.selectedLeague);\r\n      }\r\n      if (this.filterText) {\r\n        filtered = filtered.filter(game =>\r\n          game.game.toLowerCase().includes(this.filterText.toLowerCase())\r\n        );\r\n      }\r\n      return filtered;\r\n    }\r\n  },\r\n  methods: {\r\n    formatDate(dateString) {\r\n      const date = new Date(dateString);\r\n      return date.toDateString();\r\n    },\r\n    async deleteEvent(game) {\r\n      console.log(game);\r\n      let urll = `${API_URL}/event/${game}/`;\r\n      console.log(urll);\r\n      try {\r\n        const response = await axios.delete(urll);\r\n        console.log(response);\r\n      } catch (error) {\r\n        console.error('Error fetching games:', error);\r\n      }\r\n      location.reload();\r\n    },\r\n    async editEvent(game) {\r\n      this.$router.push(`/edit_event/${game}`);\r\n    },\r\n    extractLeagues() {\r\n      // Extract unique league names from games data\r\n      this.leagues = Array.from(new Set(this.games.map(game => game.league)));\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\nth,\r\ntd {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\nth {\r\n  background-color: #f2f2f2;\r\n}\r\n\r\na {\r\n  color: #0000ee;\r\n  text-decoration: underline;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":";AAiGA,OAAOA,KAAI,MAAO,OAAO;AACzB,MAAMC,OAAM,GAAI,uBAAuB;AAEvC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,EAAE;MACdC,cAAc,EAAE,EAAE;MAClBC,OAAO,EAAE,EAAC,CAAE;IACd,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAE,GAAET,OAAQ,aAAY,EAAE;QACxDU,OAAO,EAAE;UACP,cAAc,EAAE;QAClB;MACF,CAAC,CAAC;MACF,IAAI,CAACP,KAAI,GAAIK,QAAQ,CAACN,IAAI;MAC1B,IAAI,CAACS,cAAc,CAAC,CAAC,EAAE;IACzB,EAAE,OAAOC,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;IAC/C;EACF,CAAC;EACDE,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,IAAIC,QAAO,GAAI,IAAI,CAACb,KAAK;MACzB,IAAI,IAAI,CAACE,cAAc,EAAE;QACvBW,QAAO,GAAIA,QAAQ,CAACC,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,IAAI,CAACd,cAAc,CAAC;MACzE;MACA,IAAI,IAAI,CAACD,UAAU,EAAE;QACnBY,QAAO,GAAIA,QAAQ,CAACC,MAAM,CAACC,IAAG,IAC5BA,IAAI,CAACA,IAAI,CAACE,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACjB,UAAU,CAACgB,WAAW,CAAC,CAAC,CAChE,CAAC;MACH;MACA,OAAOJ,QAAQ;IACjB;EACF,CAAC;EACDM,OAAO,EAAE;IAC